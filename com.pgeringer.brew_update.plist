    <dict>
        <key>Label</key>
        <string>com.pgeringer.brew_update</string>
        <key>Program</key>
        <string>/Users/pgeringer/lib/brew_update</string>
        <key>EnvironmentVariables</key>
        <dict>
            <key>PATH</key>
            <string>/usr/local/bin:/bin:/usr/bin</string>
        </dict>
        <key>StandardOutPath</key>
        <string>/tmp/brew_update.stdout</string>
